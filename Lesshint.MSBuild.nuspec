<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Lesshint.MSBuild</id>
    <version>0.1.0</version>
    <authors>joshuakgoldberg</authors>
    <owners>joshuakgoldberg</owners>
