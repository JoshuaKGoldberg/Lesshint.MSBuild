    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      An MSBuild target for linting Less code using Lesshint.
    </description>
    <tags>less, lesshint, msbuild</tags>
    <dependencies>
      <dependency id="lesshint" version="0.1.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="dist\Lesshint.MSBuild.targets" target="build" />

    <file src="dist\LICENSE.md" target="tools" />
